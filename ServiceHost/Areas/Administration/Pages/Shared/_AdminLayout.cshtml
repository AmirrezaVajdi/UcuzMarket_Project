<!DOCTYPE html>
<html lang="fa"
      class="light-style layout-navbar-fixed layout-menu-fixed"
      dir="rtl"
      data-theme="theme-default"
      data-assets-path="AdminTheme/assets/"
      data-template="vertical-menu-template">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="author" content="AmirReza Vajdi">


    <title>@ViewData["Title"] | اوجوز مارکت</title>
    <meta name="keywords" content="" />
    <meta name="description" content="" />

    <!-- Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Public+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&display=swap" rel="stylesheet" />

    <link rel="stylesheet" href="~/AdminTheme/assets/vendor/fonts/boxicons.css" />
    <link rel="stylesheet" href="~/AdminTheme/assets/vendor/fonts/fontawesome.css" />

    <!-- Core CSS -->
    <link rel="stylesheet" href="~/AdminTheme/assets/vendor/css/rtl/core.css" class="template-customizer-core-css" />
    <link rel="stylesheet" href="~/AdminTheme/assets/vendor/css/rtl/theme-default.css" class="template-customizer-theme-css" />
    <link rel="stylesheet" href="~/AdminTheme/assets/css/demo.css" />

    <!-- Vendors CSS -->
    <link rel="stylesheet" href="~/AdminTheme/assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.css" />


    <link rel="stylesheet" href="~/AdminTheme/assets/vendor/libs/typeahead-js/typeahead.css" />
    <link rel="stylesheet" href="~/AdminTheme/assets/vendor/libs/datatables-bs5/datatables.bootstrap5.css">
    <link rel="stylesheet" href="~/AdminTheme/assets/vendor/libs/datatables-responsive-bs5/responsive.bootstrap5.css">
    <link rel="stylesheet" href="~/AdminTheme/assets/vendor/libs/flatpickr/flatpickr.css" />


    <link rel="stylesheet" href="~/AdminTheme/assets/vendor/libs/toastr/toastr.css" />
    <link rel="stylesheet" href="~/AdminTheme/assets/vendor/libs/animate-css/animate.css" />

    <!-- Vendor Theme-->
    <link rel="stylesheet" href="~/AdminTheme/assets/vendor/css/rtl/core-dark.css" />
    <link rel="stylesheet" href="~/AdminTheme/assets/vendor/css/rtl/theme-semi-dark.css" />


    <!-- Helpers -->
    <script src="~/AdminTheme/assets/vendor/js/helpers.js"></script>
    <script src="~/AdminTheme/assets/vendor/js/template-customizer.js"></script>
    <script src="~/AdminTheme/assets/js/config.js"></script>

    @RenderSection("css", false)

</head>

<div class="layout-wrapper layout-content-navbar">
    <div class="layout-container">
        <!-- Menu -->
        <partial name="_Menu" />
        <!-- / Menu -->
        <!-- Layout container -->
        <div class="layout-page">
            <!-- Navbar -->
            <partial name="_Nav" />
            <!-- / Navbar -->
            <!-- Content wrapper -->
            <div class="content-wrapper">
                <!-- Content -->

                <div class="container-xxl flex-grow-1 container-p-y">
                    @RenderBody()
                </div>
                <!-- / Content -->
                <!-- Footer -->
                <partial name="_Footer" />
                <!-- / Footer -->

                <div class="content-backdrop fade"></div>
            </div>
            <!-- Content wrapper -->
        </div>
        <!-- / Layout page -->
    </div>

    <!-- Overlay -->
    <div class="layout-overlay layout-menu-toggle"></div>

    <!-- Drag Target Area To SlideIn Menu On Small Screens -->
    <div class="drag-target"></div>
</div>

<!-- Core JS -->
<!-- build:js ~/AdminTheme/assets/vendor/js/core.js -->
<script src="~/AdminTheme/assets/vendor/libs/jquery/jquery.js"></script>
<script src="~/AdminTheme/assets/vendor/libs/popper/popper.js"></script>
<script src="~/AdminTheme/assets/vendor/js/bootstrap.js"></script>
<script src="~/AdminTheme/assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.js"></script>

<script src="~/AdminTheme/assets/vendor/libs/hammer/hammer.js"></script>
<script src="~/AdminTheme/assets/vendor/libs/typeahead-js/typeahead.js"></script>

<script src="~/AdminTheme/assets/vendor/js/menu.js"></script>
<!-- endbuild -->
<!-- Vendors JS -->
<script src="~/AdminTheme/assets/vendor/libs/apex-charts/apexcharts.js"></script>


<script src="~/AdminTheme/assets/vendor/libs/datatables/jquery.dataTables.js"></script>
<script src="~/AdminTheme/assets/vendor/libs/datatables-bs5/datatables-bootstrap5.js"></script>
<script src="~/AdminTheme/assets/vendor/libs/datatables-responsive/datatables.responsive.js"></script>
<script src="~/AdminTheme/assets/vendor/libs/datatables-responsive-bs5/responsive.bootstrap5.js"></script>
<script src="~/AdminTheme/assets/js/site.js"></script>

<script src="~/AdminTheme/assets/vendor/libs/toastr/toastr.js"></script>

<!-- Main JS -->
<script src="~/AdminTheme/assets/js/main.js"></script>

<!-- Page JS -->
<script src="~/AdminTheme/assets/js/dashboards-analytics.js"></script>

<!--Validation Scripts-->
<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>

@RenderSection("Scripts" , false)

</html>